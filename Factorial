import java.util.Scanner;

class Main{
    public static void main (String args[]){
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter your number");
        int num;
        num=sc.nextInt();
        int abc=1;
        for(int i=1; i<=num; i++){
            abc= abc * i;
        }
        System.out.println("Factorial of your number is:" + abc);
    }
}
