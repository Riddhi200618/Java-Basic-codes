import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int sum=0;
        int max=5;
        int[] arr=new int [max];
        for (int i=0; i<5; i++){
            System.out.println("Enter your "+(i+1)+"st Number:");
            arr[i]=sc.nextInt();
        }
        for(int i=0; i<5; i++){
            sum=sum+arr[i];
        }
        System.out.println("Sum of your digits is:"+sum);
    }
}
